#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,x,y;
	cin>>t;
	while(t--){
	    int count =0;
	    cin>>n>>x>>y;
	    for(int i=0;i<=n;i++){
	        if(x*i==y){
	            cout<<"YES"<<endl;
	            count=1;
	            break;
	        }
	    }
	    if(count==0){
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
